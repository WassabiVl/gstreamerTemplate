# gst-webcam-input.conf

camera_width = 176
camera_height = 144
v4l2_devname = /dev/video0
uinput_devname = /dev/uinput
input_drivername = "@DEFAULT_INPUT_DRIVER@"
learn_background_counter = 15
threshold = 25
smooth = 2
highpass_blur = 16
highpass_noise = 4
amplify = 16
surface_min = 20
surface_max = 500
distance_max = 20

