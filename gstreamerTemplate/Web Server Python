##the sender
./surface-streams/realsense -gstpipe "alpha method=green ! videorate ! video/x-raw, framerate=60/1  ! jpegenc quality=50 ! rtpgstpay config-interval=1  ! udpsink host=141.54.52.151 port=8880 sync=false"

## the reciver
gst-launch-1.0 -v udpsrc port=6000  caps="application/x-rtp, framerate=60/1, width=500, height=300" buffer-size=100000 ! rtpgstdepay ! jpegdec ! videoconvert ! xvimagesink sync=false

gst-launch-1.0 -v udpsrc port=6000  caps="application/x-rtp, framerate=60/1, width=500, height=300" ! rtpjitterbuffer ! rtpgstdepay ! jpegdec ! videoconvert ! xvimagesink sync=false


./surface-streams/realsense -gstpipe  "videorate ! video/x-raw,framerate=60/1, width=500, height=300 ! jpegenc quality=75 ! videoconvert ! autovideosink"
